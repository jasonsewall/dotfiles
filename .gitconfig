[include]
  path = .gitconfig.local
[push]
	default = matching
[color]
  diff = auto
  status = auto
  branch = auto
	interactive = auto
[clean]
	requireForce = true
[rerere]
	enabled = true
[init]
	defaultBranch = main
[core]
	excludesfile = ~/.gitignore
[url "ssh://git@gitlab-master.nvidia.com:12051/"]
	insteadOf = https://gitlab-master.nvidia.com/
